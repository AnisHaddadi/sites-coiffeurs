{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/salons",
      "destination": "/salons.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/expertises",
      "destination": "/expertises.html"
    },
    {
      "source": "/avis",
      "destination": "/avis.html"
    },
    {
      "source": "/mentions-legales",
      "destination": "/mentions-legales.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

